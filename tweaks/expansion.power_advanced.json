{
  "items": [
    {
      "id": "exp.power.duplicate_ultimate",
      "type": "action",
      "name": "Enable Ultimate Performance Plan",
      "category": "Expansion / Power Advanced",
      "risk": "Warning",
      "description": "[Focus: Latency / Input Delay] Duplicates Ultimate Performance scheme and sets it active (desktop only).",
      "requiresAdmin": true,
      "requiresReboot": false,
      "apply": {
        "steps": [
          {
            "type": "cmd.run",
            "command": "powercfg -duplicatescheme e9a42b02-d5df-448d-aa00-03f14749eb61"
          },
          {
            "type": "cmd.run",
            "command": "powercfg -setactive e9a42b02-d5df-448d-aa00-03f14749eb61"
          }
        ]
      },
      "revert": {
        "steps": []
      },
      "riskLevel": "Warning"
    },
    {
      "id": "exp.power.usb_selective_suspend_off",
      "type": "action",
      "name": "USB Selective Suspend OFF (AC)",
      "category": "Expansion / Power Advanced",
      "risk": "Warning",
      "description": "[Focus: Latency / Input Delay] Sets USB selective suspend to disabled on AC for current scheme.",
      "requiresAdmin": true,
      "requiresReboot": false,
      "apply": {
        "steps": [
          {
            "type": "cmd.run",
            "command": "powercfg -setacvalueindex SCHEME_CURRENT SUB_USB USBSELECTIVE 0"
          },
          {
            "type": "cmd.run",
            "command": "powercfg -SetActive SCHEME_CURRENT"
          }
        ]
      },
      "revert": {
        "steps": []
      },
      "riskLevel": "Warning"
    },
    {
      "id": "exp.power.pcie_link_state_off",
      "type": "action",
      "name": "PCIe Link State OFF (AC)",
      "category": "Expansion / Power Advanced",
      "risk": "Warning",
      "description": "[Focus: Latency / Input Delay] Sets PCIe Link State Power Management to Off on AC for current scheme.",
      "requiresAdmin": true,
      "requiresReboot": false,
      "apply": {
        "steps": [
          {
            "type": "cmd.run",
            "command": "powercfg -setacvalueindex SCHEME_CURRENT SUB_PCIEXPRESS ASPM 0"
          },
          {
            "type": "cmd.run",
            "command": "powercfg -SetActive SCHEME_CURRENT"
          }
        ]
      },
      "revert": {
        "steps": []
      },
      "riskLevel": "Warning"
    },
