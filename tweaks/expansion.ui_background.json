{
  "items": [
    {
      "id": "exp.ui.disable_transparency",
      "type": "toggle",
      "name": "Disable Transparency Effects",
      "category": "Expansion / UI & Background",
      "risk": "Safe",
      "description": "[Focus: FPS, Latency / Input Delay] Disables transparency effects for current user.",
      "requiresAdmin": false,
      "requiresReboot": false,
      "apply": {
        "steps": [
          {
            "type": "registry.set",
            "path": "HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Themes\\Personalize",
            "name": "EnableTransparency",
            "value": 0,
            "valueType": "DWord"
          }
        ]
      },
      "revert": {
        "steps": [
          {
            "type": "registry.set",
            "path": "HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Themes\\Personalize",
            "name": "EnableTransparency",
            "value": 1,
            "valueType": "DWord"
          }
        ]
      },
      "riskLevel": "Safe",
      "check": {
        "steps": [
          {
            "type": "registry.check",
            "path": "HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Themes\\Personalize",
            "name": "EnableTransparency",
            "valueType": "DWord",
            "equals": 0
          }
        ]
      }
    },
    {
      "id": "exp.ui.open_performance_options",
      "type": "action",
      "name": "Open Performance Options (Visual Effects)",
      "category": "Expansion / UI & Background",
      "risk": "Safe",
      "description": "[Focus: FPS, Latency / Input Delay] Opens SystemPropertiesPerformance.exe for visual effects tuning.",
      "requiresAdmin": false,
      "requiresReboot": false,
      "apply": {
        "steps": [
          {
            "type": "run.exe",
            "file": "SystemPropertiesPerformance.exe",
            "args": [],
            "wait": false
          }
        ]
      },
      "revert": {
        "steps": []
      },
      "riskLevel": "Safe"
    },
    {
      "id": "exp.ui.open_startup_apps",
      "type": "action",
      "name": "Open Startup Apps",
      "category": "Expansion / UI & Background",
      "risk": "Safe",
      "description": "[Focus: FPS, Latency / Input Delay] Opens Startup Apps settings page.",
      "requiresAdmin": false,
      "requiresReboot": false,
      "apply": {
        "steps": [
          {
            "type": "cmd.run",
            "command": "start ms-settings:startupapps"
          }
        ]
      },
      "revert": {
