    {
      "id": "exp.boot.report_bcd_current",
      "type": "action",
      "name": "Generate BCD Report (current)",
      "category": "Expansion / Boot & Timer (BCDEdit)",
      "risk": "Safe",
      "description": "[Focus: Latency / Input Delay] Saves bcdedit /enum {current} output to %ProgramData%\\FalconOptimizer\\Reports\\bcd_current.txt and opens it.",
      "requiresAdmin": true,
      "requiresReboot": false,
      "apply": {
        "steps": [
          {
            "type": "ps.run",
            "command": "$dir=Join-Path $env:ProgramData 'FalconOptimizer\\Reports'; New-Item -ItemType Directory -Force -Path $dir | Out-Null; bcdedit /enum {current} | Out-File -Encoding utf8 (Join-Path $dir 'bcd_current.txt');"
          },
          {
            "type": "open.file",
            "path": "%ProgramData%\\FalconOptimizer\\Reports\\bcd_current.txt"
          }
        ]
      },
      "revert": {
        "steps": []
      },
      "riskLevel": "Safe"
    },
    {
