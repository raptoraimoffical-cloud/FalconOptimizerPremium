    {
      "id": "exp.boot.disabledynamictick",
      "type": "button",
      "name": "Apply: Disable Dynamic Tick",
      "category": "Expansion / Boot & Timer (BCDEdit)",
      "risk": "High",
      "description": "[Focus: Latency / Input Delay] Disables dynamic tick. Can reduce jitter on some systems; can increase idle power. Reboot required.",
      "requiresAdmin": true,
      "requiresReboot": true,
      "apply": {
        "steps": [
          {
            "type": "cmd.run",
            "command": "bcdedit /set {current} disabledynamictick yes"
          }
        ]
      },
      "revert": {
        "steps": [
          {
            "type": "cmd.run",
            "command": "bcdedit /deletevalue {current} disabledynamictick"
          }
        ]
      },
      "riskLevel": "High"
    },
