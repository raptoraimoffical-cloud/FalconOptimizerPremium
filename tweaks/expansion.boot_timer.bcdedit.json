    {
      "id": "exp.boot.timerhelper_persistent_0p5ms",
      "type": "button",
      "name": "Apply: Timer Helper Persistent (0.5ms request)",
      "category": "Expansion / Boot & Timer (BCDEdit)",
      "risk": "Warning",
      "description": "[Focus: Latency / Input Delay] Starts the included timer-helper.ps1 to request 0.5ms while running; stop to revert.",
      "requiresAdmin": true,
      "requiresReboot": false,
      "apply": {
        "steps": [
          {
            "type": "process.start",
            "wait": false,
            "arguments": "-NoProfile -ExecutionPolicy Bypass -File %~dp0\\scripts\\timer-helper.ps1 -Resolution 5000",
            "filePath": "powershell.exe"
          }
        ]
      },
      "revert": {
        "steps": [
          {
            "type": "process.kill",
            "name": "powershell",
            "match": "timer-helper.ps1"
          }
        ]
      },
      "riskLevel": "Warning",
      "kind": "tool",
      "manualOnly": true
    },
