    {
      "id": "exp.boot.report_qpc_source",
      "type": "action",
      "name": "Report QPC/Timer Source (basic)",
      "category": "Expansion / Boot & Timer (BCDEdit)",
      "risk": "Safe",
      "description": "[Focus: Latency / Input Delay] Writes a basic timer report to %ProgramData%\\FalconOptimizer\\Reports\\timer_report.txt and opens it.",
      "requiresAdmin": true,
      "requiresReboot": false,
      "apply": {
        "steps": [
          {
            "type": "ps.run",
            "command": "$dir=Join-Path $env:ProgramData 'FalconOptimizer\\Reports'; New-Item -ItemType Directory -Force -Path $dir | Out-Null; $out=Join-Path $dir 'timer_report.txt'; \"Date: $(Get-Date)\" | Out-File -Encoding utf8 $out; \"WinVer: $([Environment]::OSVersion.VersionString)\" | Add-Content $out; bcdedit /enum {current} | Add-Content $out; w32tm /query /configuration 2>$null | Add-Content $out;"
          },
          {
            "type": "open.file",
            "path": "%ProgramData%\\FalconOptimizer\\Reports\\timer_report.txt"
          }
        ]
      },
      "revert": {
        "steps": []
      },
      "riskLevel": "Safe"
