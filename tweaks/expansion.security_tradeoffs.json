    {
      "id": "exp.sec.check_vbs_status",
      "type": "action",
      "name": "Check VBS Status",
      "category": "Expansion / Security Tradeoffs",
      "risk": "Safe",
      "description": "[Focus: FPS, Latency / Input Delay] Shows virtualization-based security status (DeviceGuard).",
      "requiresAdmin": true,
      "requiresReboot": false,
      "apply": {
        "steps": [
          {
            "type": "ps.run",
            "command": "Get-CimInstance -ClassName Win32_DeviceGuard | Select-Object * | Out-Host"
          }
        ]
      },
      "revert": {
        "steps": []
      },
      "riskLevel": "Safe"
    },
