  "items": [
    {
      "id": "exp.storage.check_trim",
      "type": "action",
      "name": "Check TRIM Status",
      "category": "Expansion / Storage & I/O",
      "risk": "Safe",
      "description": "[Focus: FPS, Latency / Input Delay] Runs fsutil behavior query DisableDeleteNotify (0 means TRIM enabled).",
      "requiresAdmin": true,
      "requiresReboot": false,
      "apply": {
        "steps": [
          {
            "type": "cmd.run",
            "command": "fsutil behavior query DisableDeleteNotify"
          }
        ]
      },
      "revert": {
        "steps": []
      },
      "riskLevel": "Safe"
    },
