{
  "items": [
    {
      "id": "exp.net.show_tcp_global",
      "type": "action",
      "name": "Show TCP Global Settings",
      "category": "Expansion / Network Advanced",
      "risk": "Safe",
      "description": "[Focus: Latency / Input Delay, Ping / Network] Runs netsh int tcp show global.",
      "requiresAdmin": true,
      "requiresReboot": false,
      "apply": {
        "steps": [
          {
            "type": "cmd.run",
            "command": "netsh int tcp show global"
          }
        ]
      },
      "revert": {
        "steps": []
      },
      "riskLevel": "Safe"
    },
    {
      "id": "exp.net.ping_test_cloudflare",
      "type": "action",
      "name": "Ping Test (1.1.1.1 x20)",
      "category": "Expansion / Network Advanced",
      "risk": "Safe",
      "description": "[Focus: Latency / Input Delay, Ping / Network] Runs a simple ping test to 1.1.1.1 (20 packets).",
      "requiresAdmin": false,
      "requiresReboot": false,
      "apply": {
        "steps": [
          {
            "type": "cmd.run",
            "command": "ping -n 20 1.1.1.1"
          }
        ]
      },
      "revert": {
        "steps": []
      },
      "riskLevel": "Safe"
    },
