    {
      "id": "power_set_ultimate__windows.power",
      "type": "button",
      "name": "Apply: Set Ultimate Performance Power Plan",
      "risk": "Safe",
      "description": "[Focus: Latency / Input Delay] Activates Ultimate Performance power plan (creates if missing).\nMutually exclusive with other power plans.",
      "requiresAdmin": true,
      "apply": {
        "steps": [
          {
            "type": "cmd.run",
            "command": "powercfg -duplicatescheme e9a42b02-d5df-448d-aa00-03f14749eb61 >nul 2>&1"
          },
          {
            "type": "cmd.run",
            "command": "powercfg /setactive e9a42b02-d5df-448d-aa00-03f14749eb61"
          }
        ]
      },
      "revert": {
        "steps": [
          {
            "type": "cmd.run",
            "command": "powercfg /setactive SCHEME_BALANCED"
          }
        ]
      },
      "riskLevel": "Safe",
      "category": "General / Power",
      "conflictGroup": "powerplan"
    },
