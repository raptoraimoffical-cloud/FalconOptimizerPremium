    {
      "id": "power_disable_processor_idle__windows.power",
      "type": "toggle",
      "name": "Disable Processor Idle States (Guided)",
      "risk": "Warning",
      "description": "[Focus: Latency / Input Delay] Sets idle disable via policy where applicable.",
      "requiresAdmin": true,
      "apply": {
        "steps": [
          {
            "type": "registry.set",
            "path": "HKLM\\SYSTEM\\CurrentControlSet\\Control\\Power",
            "name": "CsEnabled",
            "value": 0,
            "valueType": "DWord"
          }
        ]
      },
      "revert": {
        "steps": [
          {
            "type": "registry.remove",
            "path": "HKLM\\SYSTEM\\CurrentControlSet\\Control\\Power",
            "name": "CsEnabled"
          }
        ]
      },
      "riskLevel": "Warning",
      "category": "General / Power",
      "check": {
        "steps": [
          {
            "type": "registry.check",
            "path": "HKLM\\SYSTEM\\CurrentControlSet\\Control\\Power",
            "name": "CsEnabled",
            "valueType": "DWord",
            "equals": 0
          }
        ]
      }
    },
