    {
      "id": "powerplan.desktop__windows.power",
      "type": "button",
      "name": "Falcon Desktop Powerplan",
      "risk": "Safe",
      "description": "[Focus: Latency / Input Delay] Sets the active plan to High performance.\nMutually exclusive with other power plans.",
      "requiresAdmin": true,
      "apply": {
        "steps": [
          {
            "type": "cmd",
            "shell": "cmd",
            "command": "powercfg /setactive SCHEME_MIN"
          }
        ]
      },
      "riskLevel": "Safe",
      "category": "General / Power",
      "conflictGroup": "powerplan",
      "note": "Converted from toggle (missing revert)."
    },
