      "requiresAdmin": true,
      "apply": {
        "steps": [
          {
            "type": "cmd.run",
            "command": "powercfg -setacvalueindex SCHEME_CURRENT SUB_USB USBSELECTSUSPEND 0"
          },
          {
            "type": "cmd.run",
            "command": "powercfg -setdcvalueindex SCHEME_CURRENT SUB_USB USBSELECTSUSPEND 0"
          },
          {
            "type": "cmd.run",
