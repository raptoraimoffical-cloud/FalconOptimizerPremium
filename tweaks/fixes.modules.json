    {
      "id": "fix.wifi",
      "type": "button",
      "name": "Wi\u2011Fi / Network Restore",
      "risk": "Safe",
      "riskLevel": "Safe",
      "description": "Restores core networking services and resets IP/DNS stacks. Requires restart for best results.",
      "requiresAdmin": true,
      "category": "Network",
      "apply": {
        "steps": [
          {
            "type": "service.startup",
            "name": "LanmanWorkstation",
            "startType": "Manual"
          },
          {
            "type": "service.startup",
            "name": "WdiServiceHost",
            "startType": "Manual"
          },
          {
            "type": "service.startup",
            "name": "NcbService",
            "startType": "Manual"
          },
          {
            "type": "service.startup",
            "name": "ndu",
            "startType": "Manual"
          },
          {
            "type": "service.startup",
            "name": "Netman",
            "startType": "Manual"
          },
          {
            "type": "service.startup",
            "name": "netprofm",
            "startType": "Manual"
          },
          {
            "type": "service.startup",
            "name": "WwanSvc",
            "startType": "Manual"
          },
          {
            "type": "service.startup",
            "name": "Dhcp",
            "startType": "Automatic",
            "start": true
          },
          {
            "type": "service.startup",
            "name": "DPS",
            "startType": "Automatic",
            "start": true
          },
          {
            "type": "service.startup",
            "name": "lmhosts",
            "startType": "Automatic",
            "start": true
          },
          {
            "type": "service.startup",
            "name": "NlaSvc",
            "startType": "Automatic",
            "start": true
          },
          {
            "type": "service.startup",
            "name": "nsi",
            "startType": "Automatic",
            "start": true
          },
          {
            "type": "service.startup",
            "name": "RmSvc",
            "startType": "Automatic",
            "start": true
          },
          {
            "type": "service.startup",
            "name": "Wcmsvc",
            "startType": "Automatic",
            "start": true
          },
          {
            "type": "service.startup",
            "name": "Winmgmt",
            "startType": "Automatic",
            "start": true
          },
          {
            "type": "service.startup",
            "name": "WlanSvc",
            "startType": "Automatic",
            "start": true
          },
          {
            "type": "cmd",
            "shell": "cmd",
            "command": "reg add \"HKLM\\Software\\Policies\\Microsoft\\Windows\\NetworkConnectivityStatusIndicator\" /v \"NoActiveProbe\" /t REG_DWORD /d \"0\" /f"
          },
          {
            "type": "cmd",
            "shell": "cmd",
            "command": "reg add \"HKLM\\System\\CurrentControlSet\\Services\\NlaSvc\\Parameters\\Internet\" /v \"EnableActiveProbing\" /t REG_DWORD /d \"1\" /f"
          },
          {
            "type": "cmd",
            "shell": "cmd",
            "command": "reg add \"HKLM\\System\\CurrentControlSet\\Services\\BFE\" /v \"Start\" /t REG_DWORD /d \"2\" /f"
          },
          {
            "type": "cmd",
            "shell": "cmd",
            "command": "reg add \"HKLM\\System\\CurrentControlSet\\Services\\Dnscache\" /v \"Start\" /t REG_DWORD /d \"2\" /f"
          },
          {
            "type": "cmd",
            "shell": "cmd",
            "command": "reg add \"HKLM\\System\\CurrentControlSet\\Services\\WinHttpAutoProxySvc\" /v \"Start\" /t REG_DWORD /d \"3\" /f"
          },
          {
            "type": "cmd",
            "shell": "cmd",
            "command": "reg add \"HKLM\\SYSTEM\\CurrentControlSet\\Services\\vwifibus\" /v \"Start\" /t REG_DWORD /d \"3\" /f"
          },
          {
            "type": "cmd",
            "shell": "cmd",
            "command": "reg add \"HKLM\\SYSTEM\\CurrentControlSet\\Services\\vwififlt\" /v \"Start\" /t REG_DWORD /d \"3\" /f"
          },
          {
            "type": "cmd",
            "shell": "cmd",
            "command": "reg add \"HKLM\\SYSTEM\\CurrentControlSet\\Services\\vwifimp\" /v \"Start\" /t REG_DWORD /d \"3\" /f"
          },
          {
            "type": "cmd",
            "shell": "cmd",
            "command": "ipconfig /release"
          },
          {
            "type": "cmd",
            "shell": "cmd",
            "command": "ipconfig /renew"
          },
          {
            "type": "cmd",
            "shell": "cmd",
            "command": "arp -d *"
          },
          {
            "type": "cmd",
            "shell": "cmd",
            "command": "nbtstat -R"
          },
          {
            "type": "cmd",
            "shell": "cmd",
            "command": "nbtstat -RR"
          },
          {
            "type": "cmd",
            "shell": "cmd",
            "command": "ipconfig /flushdns"
          },
          {
            "type": "cmd",
            "shell": "cmd",
            "command": "ipconfig /registerdns"
          }
        ]
      },
      "note": "Imported from V8.3 Fixers menu (renamed)."
    },
