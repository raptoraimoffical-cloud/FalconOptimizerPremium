      "id": "registry_clean_recent_opensave__registry.lab",
      "type": "button",
      "name": "Apply: Clean RecentDocs & Open/Save MRU (HKCU)",
      "description": "[QoL + Micro-bloat] Clears RecentDocs and Open/Save dialog MRU entries under HKCU.",
      "requiresAdmin": false,
      "riskLevel": "Warning",
      "category": "Registry Lab / Cleanup",
      "apply": {
        "steps": [
          {
            "type": "ps.run",
            "command": "Write-Output \"=== FALCON REGISTRY LAB: CLEAN OPEN/SAVE MRU ===\"\nWrite-Output \"\"\nWrite-Output \"This will clean RecentDocs and OpenSave dialog MRU entries under HKCU.\"\nWrite-Output \"Impact: clears per-user 'recent files' lists and file-type dialog history. No core OS components are removed.\"\nWrite-Output \"\"\n\n$targets = @(\n  \"HKCU:\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Explorer\\RecentDocs\",\n  \"HKCU:\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Explorer\\ComDlg32\\OpenSavePidlMRU\"\n)\n\nforeach ($t in $targets) {\n  try {\n    if (Test-Path $t) {\n      Remove-Item -Path $t -Recurse -Force -ErrorAction SilentlyContinue\n      Write-Output (\"Cleared: {0}\" -f $t)\n    } else {\n      Write-Output (\"Skipped (not present): {0}\" -f $t)\n    }\n  } catch {\n    Write-Output (\"Failed to clear {0}: {1}\" -f $t, $_.Exception.Message)\n  }\n}\n\nWrite-Output \"\"\nWrite-Output \"A logoff/logon or Explorer restart can reset recent-items views after this cleanup.\"\nWrite-Output \"=== END OF OPEN/SAVE MRU CLEAN ===\""
          }
        ]
      },
      "revert": {
        "steps": [
          {
            "type": "ps.run",
            "command": "Write-Output @\"\nRecentDocs/OpenSave MRU will repopulate naturally as you open files; there is no direct revert.\n\"@"
          }
        ]
      }
    },
