    {
      "id": "reg_perf_systemresponsiveness__registry.lab",
      "type": "toggle",
      "name": "Set SystemResponsiveness",
      "description": "[Focus: FPS] Reduce background responsiveness reservation for gaming.",
      "requiresAdmin": true,
      "riskLevel": "Caution",
      "category": "Registry Lab / Performance",
      "apply": {
        "steps": [
          {
            "type": "registry.set",
            "path": "HKLM\\\\SOFTWARE\\\\Microsoft\\\\Windows NT\\\\CurrentVersion\\\\Multimedia\\\\SystemProfile",
            "name": "SystemResponsiveness",
            "value": 0,
            "valueType": "DWORD"
          }
        ]
      },
      "revert": {
        "steps": [
          {
            "type": "registry.remove",
            "path": "HKLM\\\\SOFTWARE\\\\Microsoft\\\\Windows NT\\\\CurrentVersion\\\\Multimedia\\\\SystemProfile",
            "name": "SystemResponsiveness"
          }
        ]
      },
      "check": {
        "steps": [
          {
            "type": "registry.check",
            "path": "HKLM\\\\SOFTWARE\\\\Microsoft\\\\Windows NT\\\\CurrentVersion\\\\Multimedia\\\\SystemProfile",
            "name": "SystemResponsiveness",
            "valueType": "DWORD",
            "equals": 0
          }
        ]
      }
    },
