{
  "items": [
    {
      "id": "registry_overview_analyzer__registry.lab",
      "type": "action",
      "name": "Analyze Explorer History / MRU Bloat",
      "description": "[Analyzer] Counts values/subkeys in common HKCU Explorer history zones (RunMRU, RecentDocs, OpenSaveMru, etc.).",
      "requiresAdmin": false,
      "riskLevel": "Safe",
      "category": "Registry Lab / Analyzer",
      "apply": {
        "steps": [
          {
            "type": "ps.run",
            "command": "Write-Output \"=== FALCON REGISTRY LAB: OVERVIEW ANALYZER ===\"\nWrite-Output \"\"\nWrite-Output \"Scanning a few common bloat / history zones in HKCU to estimate how dirty Explorer history is.\"\nWrite-Output \"\"\n\n$zones = @(\n  @{ Name = \"RunMRU\";        Path = \"HKCU:\\Software\\Microsoft\\Windows\\CurrentVersion\\Explorer\\RunMRU\" },\n  @{ Name = \"RecentDocs\";    Path = \"HKCU:\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Explorer\\RecentDocs\" },\n  @{ Name = \"OpenSaveMRU\";   Path = \"HKCU:\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Explorer\\ComDlg32\\OpenSavePidlMRU\" },\n  @{ Name = \"TypedPaths\";    Path = \"HKCU:\\Software\\Microsoft\\Windows\\CurrentVersion\\Explorer\\TypedPaths\" },\n  @{ Name = \"WordWheelQuery\";Path = \"HKCU:\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Explorer\\WordWheelQuery\" }\n)\n\nforeach ($z in $zones) {\n  $name = $z.Name\n  $path = $z.Path\n  try {\n    if (Test-Path $path) {\n      $key  = Get-Item $path -ErrorAction SilentlyContinue\n      $vals = $key.Property | Where-Object { $_ -ne \"(default)\" }\n      $sub  = Get-ChildItem $path -ErrorAction SilentlyContinue\n      $valCount = $vals.Count\n      $subCount = if ($sub) { $sub.Count } else { 0 }\n      Write-Output (\"{0}: Values={1}, Subkeys={2} ({3})\" -f $name, $valCount, $subCount, $path)\n    } else {\n      Write-Output (\"{0}: Not present ({1})\" -f $name, $path)\n    }\n  } catch {\n    Write-Output (\"{0}: Error reading {1}: {2}\" -f $name, $path, $_.Exception.Message)\n  }\n}\n\nWrite-Output \"\"\nWrite-Output \"These zones are where Explorer and dialogs store run history, open/save history, and search suggestions.\"\nWrite-Output \"Cleaning them can reset Explorer/file-dialog history and slightly reduce registry bloat; it will not magically add FPS.\"\nWrite-Output \"\"\nWrite-Output \"=== END OF REGISTRY LAB OVERVIEW ===\""
          }
        ]
      },
      "revert": {
        "steps": [
          {
            "type": "ps.run",
            "command": "Write-Output @\"\nAnalyzer is read-only and needs no revert.\n\"@"
          }
        ]
      }
    },
    {
      "id": "registry_clean_explorer_history__registry.lab",
      "type": "button",
      "name": "Apply: Clean Explorer Run/Search History (HKCU)",
      "description": "[QoL + Micro-bloat] Clears Run dialog, typed paths, and Explorer search history for this user.",
      "requiresAdmin": false,
      "riskLevel": "Warning",
      "category": "Registry Lab / Cleanup",
      "apply": {
        "steps": [
          {
            "type": "ps.run",
            "command": "Write-Output \"=== FALCON REGISTRY LAB: CLEAN EXPLORER RUN HISTORY ===\"\nWrite-Output \"\"\nWrite-Output \"This will clean Run dialog history, typed paths, and search suggestions under HKCU Explorer keys.\"\nWrite-Output \"Impact: resets Explorer/Run history for this user only. No core OS components are removed.\"\nWrite-Output \"\"\n\n$targets = @(\n  \"HKCU:\\Software\\Microsoft\\Windows\\CurrentVersion\\Explorer\\RunMRU\",\n  \"HKCU:\\Software\\Microsoft\\Windows\\CurrentVersion\\Explorer\\TypedPaths\",\n  \"HKCU:\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Explorer\\WordWheelQuery\"\n)\n\nforeach ($t in $targets) {\n  try {\n    if (Test-Path $t) {\n      Remove-Item -Path $t -Recurse -Force -ErrorAction SilentlyContinue\n      Write-Output (\"Cleared: {0}\" -f $t)\n    } else {\n      Write-Output (\"Skipped (not present): {0}\" -f $t)\n    }\n  } catch {\n    Write-Output (\"Failed to clear {0}: {1}\" -f $t, $_.Exception.Message)\n  }\n}\n\nWrite-Output \"\"\nWrite-Output \"Explorer will recreate needed keys as you use it. A logoff/logon or Explorer restart can refresh behavior.\"\nWrite-Output \"=== END OF EXPLORER RUN HISTORY CLEAN ===\""
          }
        ]
      },
      "revert": {
        "steps": [
          {
            "type": "ps.run",
            "command": "Write-Output @\"\nHistory will repopulate as you use Explorer/Run; there is no direct revert for deleted MRU entries.\n\"@"
          }
        ]
      }
    },
    {
      "description": "[QoL + Micro-bloat] Clears Run dialog, typed paths, and Explorer search history for this user.",
      "requiresAdmin": false,
      "riskLevel": "Warning",
      "category": "Registry Lab / Cleanup",
      "id": "registry_clean_explorer_history__registry.lab__revert",
      "type": "button",
      "name": "Revert: Clean Explorer Run/Search History (HKCU)",
      "apply": {
        "steps": [
          {
            "type": "ps.run",
            "command": "Write-Output @\"\nHistory will repopulate as you use Explorer/Run; there is no direct revert for deleted MRU entries.\n\"@"
          }
        ]
      }
    },
